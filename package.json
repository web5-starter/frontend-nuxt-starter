    "@pinia/nuxt": "^0.10.1",
    "@unocss/eslint-config": "^66.0.0",
    "@unocss/nuxt": "^66.0.0",
    "@unocss/reset": "^66.0.0",
    "@vueuse/core": "^12.7.0",
    "@vueuse/nuxt": "^12.7.0",
    "bumpp": "^10.0.3",
    "eslint": "^9.18.0",
    "lint-staged": "^15.4.3",
    "lodash-imports": "^0.0.3",
    "sass-embedded": "^1.85.0",
